(local devmode false)
(var veloX 0)
(var veloY 0)
(local p {"x" 20
					"y" 20
					"spr" 256})
				
(local rooms [])

(for [x 0 (- 240 30) 30]
	(for [y 0 (- 136 17) 17]
		(table.insert rooms [x y])))
		
(fn rget [i]
	(values (. (. rooms i) 1) (. (. rooms i) 2) 30 17))
								
(fn hmget [x y]
		(mget (// x 8) (// y 8))
)

(fn player-col-horizontal [flag]
		(or (fget (hmget (+ p.x veloX) (+ p.y veloY)) flag) (fget (hmget (+ p.x veloX 7) (+ p.y veloY)) flag) (fget (hmget (+ p.x veloX) (+ p.y veloY 7)) flag) (fget (hmget (+ p.x veloX 7) (+ p.y veloY 7)) flag)))
		
(fn player-col-bottom [flag]
		(or (fget (hmget (+ p.x veloX) (+ p.y veloY 7)) flag) (fget (hmget (+ p.x veloX 7) (+ p.y veloY 7)) flag)))

(fn player-col-top [flag]
		(and (< veloY 0) (or (fget (hmget (+ p.x veloX) (+ p.y veloY)) flag) (fget (hmget (+ p.x veloX 7) (+ p.y veloY)) flag))))
;;(music 0 1 0 true)

(fn UPDATE [] ;; update function
		(set veloX 0)
		(set veloY 0)
		
		(if (btn 0)
			(set veloY -1))
		
		(if (btn 1)
			(set veloY 1))
		
		(if (btn 2)
			(set veloX -1)
			(set p.spr 256))
		
		(if (btn 3)
			(set veloX 1)
			(set p.spr 257))
			
			
		(when (player-col-horizontal 0)
			(set veloX 0))
			
		(when (player-col-bottom 0)
			(set veloY 0))
			
		(when (player-col-top 0)
			(set veloY 0))
			
		(set p.x (+ p.x veloX))
		(set p.y (+ p.y veloY))
					
;;		(if (> p.x 230)
;;		 (set p.y 230))

	
;;		(if (> p.y 128)
;;			(set p.y 128))
			
			(when devmode
				(print p.x 0 0 12)
				(print p.y 20 0 12)))


(fn _G.TIC [] ;; Draw/Update
		(cls)
		(UPDATE)
		(map (rget 2))
		
;;		(circ 120 68 size 4)
	 (spr p.spr p.x p.y 15))
